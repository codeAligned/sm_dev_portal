{
  "name": "stratus-meridian/sm_dev_portal",
  "description": "Stratus Meridian Developer portal for Google Cloud Apigee.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": "^7.1",
    "cweagans/composer-patches": "~1",
    "drupal-composer/drupal-scaffold": "^2",
    "drupal/admin_toolbar": "^1.0",
    "drupal/adminimal_admin_toolbar": "^1.9",
    "drupal/apigee_edge": "1.x-dev",
    "drupal/better_exposed_filters": "^3.0@alpha",
    "drupal/blog": "2.x-dev",
    "drupal/bootstrap_barrio": "^4.22",
    "drupal/core": "^8.6.0",
    "drupal/ctools": "^3.1",
    "drupal/default_content": "^1.0@alpha",
    "drupal/material_admin": "^1.0@alpha",
    "drupal/openapi_ui": "^1.0@RC",
    "drupal/openapi_ui_redoc": "^1.0@RC",
    "drupal/openapi_ui_swagger": "^1.0@RC",
    "drupal/pathauto": "^1.3",
    "drupal/swagger_ui_formatter": "^2.1",
    "drupal/token": "^1.5"
  },
  "require-dev": {
    "drush/drush": "~9",
    "drupal/coder": "~8"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": {
      "drupal/core": "dist"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "extra": {
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "enable-patching": true
  }
}
